// two sum 2
class Solution {
    public int[] twoSum(int[] numbers, int target) {
        int left=0,n=numbers.length,right=n-1,sum=0;
        int arr[]=new int[2];
        while(left<right)
        {
            sum=numbers[left]+numbers[right];
            if(sum==target)
            {
                arr[0]=left+1;
                arr[1]=right+1;
                return arr;
            }
            else if(sum<target)
            left++;
            else
            right--;
        }
        return arr;

    }
}